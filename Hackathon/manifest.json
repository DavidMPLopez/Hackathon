{
    "name": "Garlic",
    "description": "David and Mina's project",
    "version": "1.0",
    "manifest_version": 3,
    "options_page": "options.html",
    "background": {
        "service_worker" : "background.js"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "all_frames": true,
        "js":      ["content.js"]
      }],
    "permissions": ["storage","activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/cutie.png",
            "32": "images/cutie.png",
            "48" : "images/cutie.png",
            "128": "images/cutie.png"
        }
    },
    "icons": {
        "16": "images/cutie.png",
        "32": "images/cutie.png",
        "48" : "images/cutie.png",
        "128": "images/cutie.png"
    }
}